{
  "name": "vr-360-player",
  "version": "1.0.0",
  "description": "Virtual reality 360 degree player for 36 Technology Ltd, INC. by Trutoo AB.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --content-base build --inline --progress --host 0.0.0.0 --port 9202",
    "prestart": "rimraf build && mkdir -p build/assets/ && cp -r src/assets/tiles build/assets",
    "build:dev": "rimraf build && webpack --progress --profile --env.development",
    "build:prod": "rimraf build && webpack --progress --profile --bail --env.production",
    "deploy": "npm run build:prod && ./deploy.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://Swiftwork@github.com/Swiftwork/vr-360-player.git"
  },
  "keywords": [
    "360",
    "VR",
    "Virtual",
    "Reality",
    "Trutoo",
    "36",
    "Technology"
  ],
  "author": "Erik Hughes",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Swiftwork/vr-360-player/issues"
  },
  "homepage": "https://github.com/Swiftwork/vr-360-player#readme",
  "devDependencies": {
    "@types/core-js": "^0.9.34",
    "@types/node": "^6.0.46",
    "awesome-typescript-loader": "^2.2.4",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "ify-loader": "^1.0.3",
    "node-sass": "^3.11.2",
    "path": "^0.12.7",
    "pug": "^2.0.0-beta6",
    "pug-loader": "^2.3.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "typedoc": "^0.5.1",
    "typedoc-webpack-plugin": "^1.1.3",
    "typescript": "^2.0.8",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.10",
    "webpack-merge": "^0.15.0"
  },
  "dependencies": {
    "core-js": "^2.4.1",
    "marzipano": "git+https://github.com/google/marzipano.git",
    "normalize.css": "^5.0.0"
  }
}